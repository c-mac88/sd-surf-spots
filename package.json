{
  "name": "yoda",
  "version": "0.2.0",
  "description": "Manages users and authentication for Origin Code Academy",
  "main": "server/server.js",
  "engines": {
    "node": "^8.1.4"
  },
  "scripts": {
    "build": "npm run less && npm run copy && npm run copy-img && webpack --config ./webpack.dev.config.js --progress --colors --watch",
    "circle-build": "npm run less && npm run copy && npm run copy-img && webpack --config ./webpack.dev.config.js --progress --colors",
    "circle-test": "cross-env NODE_ENV=TEST mocha test/server/**/*.spec.js test/server/*.spec.js",
    "copy": "cp -R \"node_modules/monaco-editor\" \"build/monaco-editor\"",
    "copy-img": "cp -R \"client/img\" \"build/img\"",
    "debug": "cross-env NODE_ENV=development npm-run-all --parallel watch:server build",
    "heroku-postbuild": "npm run less && npm run copy && npm run copy-img && webpack -p --config ./webpack.config.js --progress",
    "start-production": "node server/server.js",
    "start": "cross-env NODE_ENV=development npm-run-all --parallel watch:server build",
    "test": "cross-env NODE_ENV=development mocha test/server/**/*.spec.js test/server/*.spec.js",
    "test-client": "jest test/client/*.spec.jsx --ci --testResultsProcessor='jest-junit'",
    "watch:server": "nodemon --inspect",
    "less": "lessc client/less/style.less build/css/style.css",
    "lint": "eslint ."
  },
  "nodemonConfig": {
    "watch": [
      "./server",
      "./common/models/"
    ],
    "ignore": [
      "test/*",
      "docs/*"
    ]
  },
  "jest-junit": {
    "output": "./coverage/test-coverage.xml"
  },
